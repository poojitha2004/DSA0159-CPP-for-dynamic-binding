#include <iostream>

int main() {
    int rows = 5; // Number of rows in the pattern

    for (int i = 1; i <= rows; ++i) { // Loop through each row
        for (int j = 1; j <= i; ++j) { // Print the number 'i', 'i' times
            std::cout << i;
        }
        std::cout << std::endl; // Move to the next line after printing each row
    }

    return 0;
}
