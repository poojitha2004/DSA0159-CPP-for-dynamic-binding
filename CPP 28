#include <iostream>

// Function to compute the GCD using Euclidean algorithm
int gcd(int a, int b) {
    // Ensure a and b are non-negative
    a = std::abs(a);
    b = std::abs(b);

    // Base case: if b is 0, GCD is a
    while (b != 0) {
        int temp = b;
        b = a % b;
        a = temp;
    }

    return a;
}

int main() {
    int num1, num2;

    // Read two integers from the user
    std::cout << "Enter two integers: ";
    std::cin >> num1 >> num2;

    // Compute and display the GCD
    int result = gcd(num1, num2);
    std::cout << "The GCD of " << num1 << " and " << num2 << " is " << result << std::endl;

    return 0;
}
