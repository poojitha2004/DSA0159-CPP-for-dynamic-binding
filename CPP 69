#include <iostream>
using namespace std;

class Vector {
private:
    double x, y;

public:
    // Constructor
    Vector(double xVal = 0.0, double yVal = 0.0) : x(xVal), y(yVal) {}

    // Overload the - operator
    Vector operator-(const Vector& other) const {
        return Vector(x - other.x, y - other.y);
    }

    // Method to display the vector
    void display() const {
        cout << "Vector(" << x << ", " << y << ")" << endl;
    }
};

int main() {
    // Create two Vector objects
    Vector v1(5.0, 7.0);
    Vector v2(2.0, 3.0);

    // Subtract the two Vector objects using the overloaded - operator
    Vector v3 = v1 - v2;

    // Display the result
    cout << "Vector 1: ";
    v1.display();
    cout << "Vector 2: ";
    v2.display();
    cout << "Result of subtraction: ";
    v3.display();

    return 0;
}
